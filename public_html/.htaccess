Options -Indexes
php_flag magic_quotes_gpc off
php_flag register_globals off
php_flag log_errors on
php_flag ignore_repeated_errors on
php_value error_log /home/vhost/zimzatik/logs/phperror_log

# Hide .svn directories from the user; return a "File Not Found" error
RedirectMatch 404 ^.*/\.svn(/.*)?$
